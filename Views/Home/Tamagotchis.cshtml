<head>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
</head>
<body>
	<h1>Welcome to Tamagotchi</h1>
	
<h3>Create Tamagotchi</h3>
    <form action="/tamagochi/create" method = "post">
        <div class="form-group">
            <label for="name">Enter Tamagotchi name here..</label>
            <input id="name" name="name" class="form-control" type="text">
        </div>
        <div class="form-group">
            <label for="food">Enter Tamagotchi food here..</label>
            <input id="food" name="food" class="form-control" type="text">
        </div>
        <div class="form-group">
            <label for="attention">Enter Tamagotchi attention here..</label>
            <input id="attention" name="attention" class="form-control" type="text">
        </div>
        <div class="form-group">
            <label for="rest">Enter Tamagotchi rest here..</label>
            <input id="rest" name="rest" class="form-control" type="text">
        </div>
      <button type="submit">Submit</button>

    </form>
</body>
